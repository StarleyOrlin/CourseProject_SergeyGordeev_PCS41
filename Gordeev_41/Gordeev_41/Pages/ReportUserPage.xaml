<Page x:Class="Gordeev_41.Pages.ReportUserPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Gordeev_41.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Отчет по клиентам">

    <Grid>
        <WebBrowser Margin="10 10 10 40" Name="BrowserReport"></WebBrowser>
        
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="5 5 10 5">
            <Button Name="ButtonPrint" Content="Печать"  Click="ButtonPrint_Click"></Button>
        </StackPanel>
    </Grid>
</Page>
