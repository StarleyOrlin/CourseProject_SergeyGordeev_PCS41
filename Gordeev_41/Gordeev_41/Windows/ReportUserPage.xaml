<Window x:Class="Gordeev_41.Windows.ReportUserPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gordeev_41.Windows"
        mc:Ignorable="d"
        Title="Отчет по клиентской базе" Height="450" MinHeight="450" Width="800" MinWidth="800">
    <Grid>
        <WebBrowser Margin="10 10 10 40" Name="BrowserReport"></WebBrowser>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5 5 10 5">
            <Button Name="ButtonPDF" Content="Сформировать отчет в PDF"  Click="ButtonPDF_Click"></Button>
        </StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="5 5 10 5">
            <Button Name="ButtonPrint" Content="Печать"  Click="ButtonPrint_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
